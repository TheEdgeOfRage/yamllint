{
  "problemMatcher": [
    {
      "owner": "yamllint",
      "pattern": [
        {
            "regexp": "^(\\..*)$",
            "file": 1
        },
        {
            "regexp": "^^\\s+(\\d+)\\:(\\d+)\\s+(\\w+)\\s+(.*)\\((.*)\\)$",
            "line": 1,
            "column": 2,
            "severity": 3,
            "message": 4,
            "code": 5,
            "loop": true
        }
      ]
    }
  ]
}
